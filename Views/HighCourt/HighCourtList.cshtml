@model List<CourtroomProject.COURTER.HighCourt>
@{
    ViewBag.Title = "HighCourtList";
}

<h2>HighCourtList</h2>


<table class="table table-hover">
    <tr>

        <th>Chief Justice</th>
        <th>Address</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Action</th>


    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ChiefJustice</td>
            <td>@item.address</td>
            <td>@item.Phoneno</td>
            <td>@item.email</td>
            
            <td>

                <a href="@Url.Action("HighCourtIndex",new { item.ID,item.ChiefJustice,item.address,item.Phoneno,item.email})" class="btn btn-success">Edit</a>
                <a href="@Url.Action("Delete_HC", new { del =item.ID})" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>